<?xml version='1.0' encoding='utf-8'?>
<widget id="com.movieApp" version="1.0.0" versionCode="10" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Movie Search</name>
    <icon src="icon.png" />
    <icon platform="android" qualifier="ldpi" src="ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="xhdpi.png" />
    <icon platform="android" qualifier="xxhdpi" src="xxhdpi.png" />
    <icon platform="android" qualifier="xxxhdpi" src="xxxhdpi.png" />
    <icon platform="android" qualifier="fr-xxhdpi" src="fr-xxhdpi.png" />
    <allow-navigation href="*://*omdbapi.com/*">
    <access origin="*" />
    <description>
      Search for movies!
  </description>
    <author>Jason Allen
  </author>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>












